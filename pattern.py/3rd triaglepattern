n=5
for i in range(1,n+1):
    print(" "*(n-i),end='')
    for j  in range(1,i+1):
        print(j,end='')
    print()    

n=5
temp=1
for i in range(1,n+1):
    print("  "*(n-i),end='')
    for j  in range(1,i+1):
        print(temp,end='')
        temp=temp+1
    print()    

